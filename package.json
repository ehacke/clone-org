{
  "name": "@ehacke/clone-org",
  "description": "Clone all org repos",
  "version": "0.0.1",
  "author": "Eric Hacke",
  "bin": {
    "clone-all": "./dist/src/clone-all.js"
  },
  "bugs": {
    "url": "https://github.com/ehacke/clone-org/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "class-validator": "^0.12.2",
    "commander": "^6.1.0",
    "conf": "^7.0.1",
    "debug": "^4.1.1",
    "err": "^2.1.11",
    "figures": "^3.2.0",
    "fs-extra": "^9.0.1",
    "getenv": "^1.0.0",
    "http-status": "^1.4.2",
    "inquirer": "^7.3.1",
    "lodash": "^4.17.19",
    "log-symbols": "^4.0.0",
    "luxon": "^1.24.1",
    "open": "^7.0.4",
    "ora": "^5.1.0",
    "os-name": "^4.0.0",
    "semver": "^7.3.2",
    "table": "^6.0.3",
    "terminal-link": "^2.1.1",
    "ts-essentials": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ehacke/commitlint-config": "^1.0.3",
    "@ehacke/eslint-config": "^1.1.6",
    "@ehacke/prettier-config": "^1.0.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bluebird": "^3.5.32",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/getenv": "^1.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.157",
    "@types/luxon": "^1.24.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.22",
    "@types/semver": "^7.3.1",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "commitlint": "^11.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "fixpack": "^3.0.6",
    "husky": "^4.2.3",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "sinon": "^9.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*.{js,ts,tsbuildinfo}",
    "dist/src/commands/templates/*",
    "docs/**/*"
  ],
  "homepage": "https://github.com/ehacke/clone-org#readme",
  "keywords": [
    "bitbucket",
    "clone",
    "github",
    "org",
    "repos"
  ],
  "license": "MIT",
  "lint-staged": {
    "./package.json": [
      "fixpack"
    ],
    "*.{js,ts}": [
      "prettier-eslint --write",
      "eslint --fix"
    ]
  },
  "main": "dist/src/clone-all.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehacke/clone-org.git"
  },
  "scripts": {
    "beta": "npm version prerelease && npm publish --tag beta",
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && sed -i 's/\"files\":/\"_files\":/' dist/package.json",
    "commit": "git-cz",
    "docs": "typedoc --plugin typedoc-plugin-markdown --readme none --exclude \"tests/**/*.+(unit|it).+(ts|js)\"",
    "global": "npm run build && npm link",
    "integration": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.it.+(ts|js)\"",
    "lint": "fixpack && prettier-eslint --write && eslint --fix \"**/*.{js,ts}\"",
    "major": "npm version major && npm publish",
    "minor": "npm version minor && npm publish",
    "patch": "npm version patch && npm publish",
    "postpublish": "git push",
    "prepare": "fixpack",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint && npm run test",
    "test": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.+(unit|it).+(ts|js)\"",
    "unit": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.unit.+(ts|js)\""
  },
  "types": "dist/src/index.d.ts"
}
